<form method="post" action="{{ path('famille_delete', {'famId': famille.famId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ famille.famId) }}">
    <button class="btn">Delete</button>
</form>