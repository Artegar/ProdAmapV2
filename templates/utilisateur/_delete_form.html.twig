<form method="post" action="{{ path('utilisateur_delete', {'utilId': utilisateur.utilId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ utilisateur.utilId) }}">
    <button class="btn">Delete</button>
</form>