<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Amap" table="amap">
    <id name="amapId" type="integer" column="amap_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="amapNom" type="string" column="amap_nom" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="amapAdresse1" type="string" column="amap_adresse1" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="amapAdresse2" type="string" column="amap_adresse2" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="amapCodepostal" type="string" column="amap_codepostal" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="amapVille" type="string" column="amap_ville" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
